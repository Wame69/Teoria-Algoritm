using System;
using System.Collections.Generic;
using System.Linq;

class Program
{
    const string RUSSIAN_ALPHABET = "абвгдеёжзийклмнопрстуфхцчшщъыьэюя";
    const string ENGLISH_ALPHABET = "abcdefghijklmnopqrstuvwxyz";

    static void Main()
    {
        string input = "рррммпппааа4"; // Вводимые буквы и цифры
        Console.WriteLine("Буквы для использования: " + input);

        // Убираем цифры из строки
        input = Regex.Replace(input, "[^a-zA-Zа-яА-Я]", "");
        Console.WriteLine($"Очищенная строка: {input}");

        // Определяем максимальное количество повторений каждой буквы
        var maxRepeats = new Dictionary<char, int>();
        foreach (char c in input)
        {
            maxRepeats[c] = 1;
        }

        Dictionary<char, int> lettersCount = new Dictionary<char, int>();
        foreach (char c in input)
        {
            if (!lettersCount.ContainsKey(c))
                lettersCount[c] = 0;
            
            lettersCount[c]++;
        }

        List<string> words = new List<string>();
        generateWords(input, "", words, lettersCount, maxRepeats);

        Console.WriteLine("\nСгенерированные слова:");
        foreach (var word in words)
        {
            Console.WriteLine(word);
        }
    }

    private static void generateWords(string input, string currentWord, List<string> words, Dictionary<char, int> lettersCount, Dictionary<char, int> maxRepeats)
    {
        if (currentWord.Length == input.Length)
        {
            words.Add(currentWord);
            return;
        }

        for (int i = 0; i < input.Length; ++i)
        {
            char currentChar = input[i];
            if (lettersCount.TryGetValue(currentChar, out int count) && count > 0)
            {
                if (count > maxRepeats[currentChar]) continue;

                lettersCount[currentChar]--;
                
                String newCurrentWord = currentWord + currentChar;
                generateWords(input, newCurrentWord, words, lettersCount, maxRepeats);
                
                lettersCount[currentChar]++;
            }
        }
    }
}
